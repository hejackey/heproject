<LinearLayout android:id="@+id/LinearLayout01"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical">
	<TextView android:id="@+id/TextView01" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="原型进度条"></TextView>
	<ProgressBar android:id="@+id/ProgressBar01"
		android:layout_width="wrap_content" android:layout_height="wrap_content"></ProgressBar>

<TextView android:id="@+id/TextView02" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="条形进度条"></TextView>
	<ProgressBar android:id="@+id/ProgressBar02"
		android:layout_height="wrap_content" style="?android:attr/progressBarStyleHorizontal"
		
		android:max="100" android:layout_width="fill_parent" android:progress="10"></ProgressBar>
</LinearLayout>
