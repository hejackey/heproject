<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" 
    "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="HelloWorld">
 
	<typeAlias alias="helloWorld" type="com.bfb.portal.model.HelloWorld"/>
	
	<select id="getHelloWorld" parameterClass="string" resultClass="helloWorld">
    <![CDATA[       
		select * from hellworld
		where id=#value#
    ]]>
    </select>
</sqlMap>
